{
  "private": true,
  "scripts": {
    "dev": "npx umi dev",
    "build": "npx umi build",
    "postinstall": "npx umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/plugin-qiankun": "^2.17.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.23",
    "antd": "^4.7.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.23",
    "umi-request": "^1.3.5",
    "yorkie": "^2.0.0"
  }
}
